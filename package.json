		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.12",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.2.2",
		"jest-environment-jsdom": "^29.2.2",
		"jsdom": "^20.0.1",
		"postcss": "^8.4.18",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.0",
		"tslib": "^2.3.1",
